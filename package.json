{
	"publisher": "MHerrmann",
	"name": "cortex-debug-dp-efr32fg23",
	"displayName": "Cortex-Debug: Device Support Pack - EFR32FG23",
	"description": "This package contains additional support functionality (SVD definitions) for Cortex-Debug for EFR32FG23 devices",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.87.0"
	},
	"categories": [
		"Debuggers"
	],
	"keywords": [
		"Cortex-Debug",
		"Cortex-Debug Device Support Pack",
		"cortex-m",
		"gdb",
		"debug",
		"embedded"
	],
